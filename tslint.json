{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ],
    "no-console": false,
    "max-classes-per-file": false,
    "arrow-parens": false,
    "forin": false,
    "radix": false
  },
  "rulesDirectory": []
}
